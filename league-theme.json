    "Description": {
      "Actions": {
        "Button": {
          "value": "Button",
          "type": "other",
          "description": "Buttons allow users to perform an action or to navigate to another page. They have multiple styles for various needs, and are ideal for calling attention to where a user needs to do something in order to move forward in a flow."
        },
        "Text Action": {
          "value": "Text Action",
          "type": "other",
          "description": "Text Actions allow users to navigate to a different location or initiate an action in a smaller form factor."
        }
      },
      "Typography & Assets": {
        "Icon": {
          "value": "Icon",
          "type": "other",
          "description": "An Icon is a graphical representation of a concept or thing. Use them to add salience to an item or serve as a representation."
        },
        "Tiny Icon": {
          "value": "Tiny Icon",
          "type": "other",
          "description": "Tiny Icons are used as meaningful decorations or replacements for actions."
        },
        "Typography": {
          "value": "Typography",
          "type": "other",
          "description": "Typography consists of Headings, UI Text and Paragraph Text. These can be presented in an emphasized or subtle style."
        }
      },
      "Status & Feedback": {
        "Skeleton": {
          "value": "Skeleton",
          "type": "other",
          "description": "Skeleton are components are used to represent UI elements while they are loading."
        },
        "Status Badge": {
          "value": "Status Badge",
          "type": "other",
          "description": "Status Badges are used to communicate the status of an item along with a semantically meaninful colour in order to draw attention."
        },
        "Status Banner": {
          "value": "Status Banner",
          "type": "other",
          "description": "Status Banners are highly salient elements that provide an important notification and status. Generally these are used after submitting a form."
        },
        "Image Message": {
          "value": "Image Message",
          "type": "other",
          "description": "An Image message is used to pair an image along with a piece of content. Commonly used for empty states and cards that have a visual associated to it. These can be presented with an image on the top, left or right."
        }
      },
      "Data Entry": {
        "Form Field": {
          "value": "Form Field",
          "type": "other",
          "description": "A Form Field allows you to compose an input field with the additional meta-information required by a form (labels, error messages, tooltips, etc.)"
        },
        "Text Field": {
          "value": "Text Field",
          "type": "other",
          "description": "A Text Field is an input type that allows a user to enter text. It can optionally be associated with decorations or additional actions."
        },
        "Select Field": {
          "value": "Select Field",
          "type": "other",
          "description": "A Select Field allows a user to select from a discrete set of actions."
        },
        "Checkboxes": {
          "value": "Checkboxes",
          "type": "other",
          "description": "Checkboxes allow users to select multiple items from a list of individual items, or to mark one individual item as selected."
        },
        "Radio": {
          "value": "Radios",
          "type": "other",
          "description": "Radios allow you to select a single option in a set of options with all options exposed without interaction."
        },
        "Answer Rows": {
          "value": "Answer Rows",
          "type": "other",
          "description": "Answer Rows allow the user to select one or multiple items described as a sentence."
        }
      },
      "Containers": {
        "Dialog": {
          "value": "Dialog",
          "type": "other",
          "description": "A Dialog displays important information that users need to acknowledge. They appear over the interface and block further interactions until an action is selected."
        },
        "Drawer": {
          "value": "Drawer",
          "type": "other",
          "description": "A Drawer is a modal container that is used to display navigation content or more complex selection actions, such as filtering."
        },
        "Box": {
          "value": "Box",
          "type": "other",
          "description": "A Box is used to contain content within a specified area, with or without a background. It can also contain an image as decoration."
        },
        "Card": {
          "value": "Card",
          "type": "other",
          "description": "A Card is used to contain content as a distinct element from the background. It can optionally display an image as decoration."
        },
        "Popover": {
          "value": "Popover",
          "type": "other",
          "description": "Popovers are containers used to display transient content such as menus, options, additional actions, and more. They visually stand out through stroke and drop shadow and float on top of the interface."
        },
        "Tabs": {
          "value": "Tabs",
          "type": "other",
          "description": "Tabs organize content into multiple sections and allow users to navigate between them. The content under the set of tabs should be related and form a coherent unit."
        },
        "Scrollable": {
          "value": "Scrollable",
          "type": "other",
          "description": "A Scollable container is used to create a fixed height or width container in which content extends beyond its bounds."
        }
      },
      "Layout": {
        "Column Layout": {
          "value": "Column Layout",
          "type": "other",
          "description": "A Column Layout is used to size containers within the layout grid."
        },
        "Stack Layout": {
          "value": "Stack Layout",
          "type": "other",
          "description": "A Stack Layout is used to space multiple items in a single direction. It behaves the same as Figma's Auto Layout."
        },
        "Divider": {
          "value": "Divider",
          "type": "other",
          "description": "Dividers bring clarity to a layout by grouping and dividing content in close proximity. They can also be used to establish rhythm and hierarchy."
        }
      },
      "Data Display": {
        "Tooltip": {
          "value": "Tooltip",
          "type": "other",
          "description": "Tooltips show contextual help or information about specific components when a user hovers or focuses on them."
        }
      },
      "Navigation": {
        "App Bar": {
          "value": "App Bar",
          "type": "other",
          "description": "An App Bar appears at the top of a page or window to allow the user to navigate or close it. Additional, contextual actions can also be included."
        }
      }
    }
